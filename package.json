{
  "name": "react-kit",
  "version": "1.0.0",
  "description": "Boilerplate for project with React and Webpack.",
  "scripts": {
    "start": "webpack-dev-server --devtool eval --progress --colors --hot --content-base build --inline --history-api-fallback",
