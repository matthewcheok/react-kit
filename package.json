    "boilerplate",
    "hot",
    "reload",
    "hmr",
    "live",
    "edit",
    "webpack"
  ],
  "author": "Matthew Cheok <react@matthewcheok.com> (http://github.com/matthewcheok)",
  "license": "MIT",
  "homepage": "https://github.com/matthewcheok/react-kit",
  "devDependencies": {
    "babel-core": "^5.4.7",
    "babel-eslint": "^3.1.9",
    "babel-loader": "^5.1.2",
    "css-loader": "^0.15.2",
    "eslint-plugin-react": "^2.3.0",
    "node-sass": "^3.2.0",
    "react-hot-loader": "^1.2.7",
    "sass-loader": "^1.0.2",
    "style-loader": "^0.12.3",
    "webpack": "^1.9.6",
    "webpack-dev-server": "^1.8.2"
  },
  "dependencies": {
    "react": "^0.13.0"
  }
